# Set this to the desired target architecture. Valid options include:
#
# - x86_64
ARCH = x86_64

# Sane defaults, feel free to configure:
HARE = hare
HAREC = harec
QBE = qbe
AS = x86_64-elf-as
LD = x86_64-elf-ld
OBJCOPY = x86_64-elf-objcopy
GDB = x86_64-elf-gdb
DTC = x86_64-elf-dtc
CC = x86_64-elf-gcc
